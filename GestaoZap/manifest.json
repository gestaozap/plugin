{
  "name": "GestaoZap",
  "version": "0.0.1",
  "author": "",
  "manifest_version": 21,
  "description": "Sistema de Gest√£o No whatsapp",
  "permissions": [
    "https://web.whatsapp.com/*"
  ],
  "web_accessible_resources":["/toolbar.html", "locales/*", "/popup.js", "whatsapp.js"
  ],
  "browser_action": {
    "default_icon": {
      "19": "logo16.png",
      "38": "logo48.png"
    }
  },
  "background": {
  "scripts": ["background.js"],
  "persistent": false
  },
  "icons": {
     "128": "logo128.png",
     "16": "logo16.png",
     "48": "logo48.png"
  },
  "content_security_policy": "script-src 'self' https://jsonplaceholder.typicode.com/ https://gestaozap-project.mybluemix.net https://ssl.google-analytics.com 'unsafe-eval'; object-src 'self'",
  "homepage_url": "http://Google.com.br",
  "content_scripts": [
    {
          "matches": [ "https://web.whatsapp.com/*"],
          "js" : [ "plugin.js"],
          "run_at" : "document_idle",
          "all_frames" : false
    }
  ]
}
